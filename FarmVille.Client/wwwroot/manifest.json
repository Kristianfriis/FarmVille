    { "src": "/assets/icons/web/icon-512.png", "type": "image/png", "sizes": "512x512" },
    { "src": "/assets/icons/web/icon-192-maskable.png", "type": "image/png", "sizes": "192x192", "purpose": "maskable" },
    { "src": "/assets/icons/web/icon-512-maskable.png", "type": "image/png", "sizes": "512x512", "purpose": "maskable" }
